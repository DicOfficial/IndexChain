! have bitcoind && {
! 
! # call $bitcoind for RPC
! _bitcoin_rpc() {
!     # determine already specified args necessary for RPC
!     local rpcargs=()
!     for i in ${COMP_LINE}; do
!         case "$i" in
!             -conf=*|-proxy*|-rpc*)
!                 rpcargs=( "${rpcargs[@]}" "$i" )
!                 ;;
!         esac
!     done
!     $bitcoind "${rpcargs[@]}" "$@"
! }
! 
! # Add bitcoin accounts to COMPREPLY
