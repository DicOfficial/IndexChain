  template<size_t Depth, typename Hash>
+ Hash IncrementalMerkleTree<Depth, Hash>::last() const {
+     if (right) {
+         return *right;
+     } else if (left) {
+         return *left;
+     } else {
+         throw std::runtime_error("tree has no cursor");
+     }
+ }
+ 
